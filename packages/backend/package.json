{
  "name": "clug-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.{js,ts}\" --fix",
    "serve": "node build/server.js",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose --runInBand --detectOpenHandles",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"tsc -w\" \"nodemon build/server.js\"",
    "watch-test": "npm run test -- --watchAll",
    "migration:generate": "typeorm migration:generate -d build/data-source.js",
    "migration:create": "typeorm migration:create -d build/data-source.js",
    "migration:run": "typeorm migration:run -d build/data-source.js",
    "migration:rollback": "typeorm migration:revert -d build/data-source.js",
    "docker:dev": "npm run build && npm run migration:run && npm run watch",
    "clug": "node build/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.81.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "class-validator": "^0.12.2",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-jwt": "^6.1.2",
    "express-jwt-permissions": "^1.3.6",
    "express-promise-router": "^4.1.1",
    "express-validator": "^6.14.0",
    "faker": "^5.5.3",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.4",
    "sql.js": "^1.6.2",
    "typeorm": "^0.3.6",
    "winston": "^3.7.2",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.2",
    "@types/eslint": "^7.29.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "0.0.42",
    "@types/faker": "^5.5.9",
    "@types/helmet": "0.0.47",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.33",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^15.0.14",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.16",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
