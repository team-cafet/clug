{
  "name": "backend-v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src/**/*.{js,ts}\" --fix",
    "serve": "node build/server.js",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose --runInBand --detectOpenHandles",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"tsc -w\" \"nodemon build/server.js\"",
    "watch-test": "npm run test -- --watchAll",
    "migration:generate": "typeorm migration:generate --config build/config/ormconfig.js",
    "migration:create": "typeorm migration:create --config build/config/ormconfig.js",
    "migration:run": "typeorm migration:run --config build/config/ormconfig.js",
    "migration:rollback": "typeorm migration:revert --config build/config/ormconfig.js",
    "docker:dev": "npm run build && npm run migration:run && npm run watch",
    "clug": "node build/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.884.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-permissions": "^1.3.3",
    "express-promise-router": "^4.0.1",
    "express-validator": "^6.6.1",
    "faker": "^5.4.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "winston": "^3.3.3",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/eslint": "^7.2.2",
    "@types/express": "^4.17.7",
    "@types/express-jwt": "0.0.42",
    "@types/faker": "^5.1.7",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@types/supertest": "^2.0.10",
    "@types/yargs": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
